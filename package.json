{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "description": "Frontend boilerplate",
  "keywords": [
    "javascript",
    "js",
    "typescript",
    "ts",
    "react",
    "jsx",
    "tsx",
    "webpack",
    "boilerplate",
    "template",
    "frontend"
  ],
  "author": "Nikita Karapuzov <karapuzov.nikita@gmail.com>",
  "license": "ISC",
  "browserslist": [
    "chrome 70",
    "last 1 versions",
    "not ie <= 8"
  ],
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "stats": "webpack --config webpack.prod.js --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "dependencies": {
    "core-js": "3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.8.3"
